#include <vector> // libary for vector
#include <iostream>
using namespace std;

int main() {
	// Vector Initialization
	std::vector<double> prices = {1, 2,3, 4, 5};
	//add values to the end of the proces vector
	prices.push_back(10);
	prices.push_back(50);
	prices.push_back(11);
	
	//acccesing and modifying
	double total = prices [0] + prices[1] + prices[2];
	
	cout << "Total = $" << total << endl;
	}
