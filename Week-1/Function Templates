//function templates
#include <iostream>

using namespace std;

//// to find the max of two values of any type
template <typename T>
T maxNum(T a, T b) {
	return (a > b) ? a : b;
}
//
int main() {
	int intMax = maxNum(5, 10); //we're using Integer and calling maxNum()
	double doubleMax = maxNum(2.5, 11.6); // using double and calling max()
	
	cout << "Integer Max: " << intMax << endl;
	cout << "Double Max: " << doubleMax << endl;
	
	return 0;
}
