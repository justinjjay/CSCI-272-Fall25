	string str1 = "capple";
	string str2 = "abanana";

	if (str1 == str2){
	cout << "Both strings are equal" << endl;
	} else if (str1 > str2){
		cout << "str1 is lexicographically greater than str2." << endl;
	} else { 
	cout << "str1 is lexicographically less than str2." << endl;
}
