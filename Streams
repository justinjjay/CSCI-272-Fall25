#include <iostream>
using namespace std;

class Card {
	public:
		int cardNumber;
		string cardHolder;
};

// type of alias cardPtr to for Card*
typedef Card* CardPtr; // CardPtr is an alias of Card*
typedef double DB; // created an alias of double 
typedef int ROY;

int main(){
	// create a new card object
	Card myCard;
	myCard.cardNumber = 12345;
	myCard.cardHolder = "Alice";
	
	CardPtr ptrToCard = &myCard;
	
	cout << "Card Number: " << ptrToCard->cardNumber << endl;
	cout << "Card Holder: " << ptrToCard->cardHolder << endl;
	
	DB num = 3.14;
	ROY ph = 000;
	
	return 0;
	
	}
